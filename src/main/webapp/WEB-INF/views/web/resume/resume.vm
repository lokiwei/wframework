#set($layout = "layout/null.vm")
<!DOCTYPE html>
<html>
<head lang="en">
	<meta charset="UTF-8">
	<title>京东招聘</title>
	<link rel="stylesheet" href="#springUrl('/campus/css/base.css')"/>
    <link href="#springUrl('/static/webuploader/webuploader.css')" rel="stylesheet" type="text/css"/>
	<link rel="stylesheet" href="#springUrl('/campus/css/resume.css')"/>
	<!--<link rel="stylesheet" href="#springUrl('/campus/css/resume-pub.css')"/>-->
	
	<script src="#springUrl('/campus/js/libs/jquery.js')"></script>
</head>
<body>

<!-- 通用顶栏 -->
#parse("/common/head.vm")

<!-- 简历头部 -->
<div class="resume-header">
	<div class="wrap">
		<ul class="resume-header-l">
			<li class="resume-my-resume resume-header-selected"><a href="#">我的简历</a></li>
			<li class="seperator">|</li>
			<li class="resume-my-application"><a href="/web/apply/myjob">我申请的职位</a></li>
		</ul>
		<div class="resume-header-r">
			<div class="resume-progress-bar-wrapper">
				<div class="resume-progress-bar" style="width: $!{fullAttr}%;"></div>
			</div>
			<div class="resume-progress-percent"><span>$!{fullAttr}</span>%<b>简历完整度</b></div>
			<input type="hidden" name="haveJob" id="haveJob" value=$!{haveJob}>
			<input type="hidden" name="haveAttachment" id="haveAttachment">
		</div>
	</div>
</div>

<!-- 简历主体 -->
<div class="wrap resume-wrap">
	<!-- 侧栏 -->
	<div class="resume-sidebar J-resume-sidebar">
		<!--<ul class="resume-sidebar-ul">
			<li class="curr" data-validate="form"><a href="javascript:;"><i class="resume-nav-info"></i>个人信息<strong>*</strong></a></li>
			
			<li data-validate="list"><a href="javascript:;"><i class="resume-nav-edu"></i>教育背景<strong>*</strong></a></li>
			
			<li data-validate="form"><a href="javascript:;"><i class="resume-nav-language"></i>语言能力<strong>*</strong></a></li>
			<li><a href="javascript:;"><i class="resume-nav-rewards"></i>奖励活动</a></li>
			<li><a href="javascript:;"><i class="resume-nav-exp"></i>实习经验</a></li>
			
			<li data-validate="list"><a href="javascript:;"><i class="resume-nav-jobs"></i>工作经验</a></li>
			<li><a href="javascript:;"><i class="resume-nav-family"></i>家庭成员</a></li>
		
			<li data-validate="form"><a href="javascript:;"><i class="resume-nav-other"></i>其他信息<strong>*</strong></a></li>
			<li><a href="javascript:;"><i class="resume-nav-attachments"></i>上传附件</a></li>
			#if($!{resumeBaseInfo.id}!="null")
				<li id="resume-preview"><a href="javascript:;" onclick="javascript:resumePreview();" class="resume-preview">预览简历</a></li>
			#else
				<li id="resume-preview" class="hide"><a href="javascript:;" onclick="javascript:resumePreview();" class="resume-preview">预览简历</a></li>
			#end	
		</ul>
		-->
		<!--S 20150311-导航调整顺序，相应内容区域也要按照此顺序做调整 -->
		<ul class="resume-sidebar-ul">
			<li class="curr" data-validate="form"><a href="javascript:;"><i class="resume-nav-info"></i>个人信息<strong>*</strong></a></li>
			<li data-validate="list"><a href="javascript:;"><i class="resume-nav-edu"></i>教育背景<strong>*</strong></a></li>
			<!--S 20150311-add -->
			<li data-validate="list"><a href="javascript:;"><i class="resume-nav-certificate"></i>专业证书</a></li>
			<!--E 20150311-add -->
			<li><a href="javascript:;"><i class="resume-nav-rewards"></i>奖励活动</a></li>
			<li><a href="javascript:;"><i class="resume-nav-exp"></i>实习经验</a></li>
			<!--S 20150311-add -->
			<li><a href="javascript:;"><i class="resume-nav-project"></i>项目经验</a></li>
			<!--E 20150311-add -->
			<li data-validate="list"><a href="javascript:;"><i class="resume-nav-jobs"></i>工作经验</a></li>
			<li data-validate="form"><a href="javascript:;"><i class="resume-nav-language"></i>语言能力<strong>*</strong></a></li>
			<li><a href="javascript:;"><i class="resume-nav-family"></i>家庭成员</a></li>
			<li data-validate="form"><a href="javascript:;"><i class="resume-nav-other"></i>其他信息<strong>*</strong></a></li>
			<li><a href="javascript:;"><i class="resume-nav-attachments"></i>上传附件</a></li>
			#if($!{resumeBaseInfo.id}!="null")
				<li id="resume-preview"><a href="javascript:;" onclick="javascript:resumePreview();" class="resume-preview">预览简历</a></li>
			#else
				<li id="resume-preview" class="hide"><a href="javascript:;" onclick="javascript:resumePreview();" class="resume-preview">预览简历</a></li>
			#end	
		</ul>
		<!--S 20150311-导航调整顺序，相应内容区域也要按照此顺序做调整 -->
		
		
	</div>

	<div id="J-resume-main" class="resume-main">
		<!-- 个人信息 -->
		<div id="J-info-part" class="resume-part resume-curr-part">
			<div class="resume-main-tit">
				个人信息 <span>（<i class="resume-required">*</i>必填）</span>
			</div>
			<div class="resume-main-content">
				<form id="resumeBaseForm" action="/web/resume/handleBaseInfo" method="post">
				<input type="hidden" name="id" id="baseId" value=$!{resumeBaseInfo.id}>
				<input type="hidden" name="photoUrl" id="photoUrl" value=$!{resumeBaseInfo.photoUrl}>
				<!-- 用户头像 -->
				<div class="resume-user-photo">
					#if($!{resumeBaseInfo.photoUrl}!="null"&&$!{resumeBaseInfo.photoUrl}!="")
						<img id="resume-upload-img" style="width:102px;height: 116px;" src="$!{resumeBaseInfo.photoUrl}" alt="">
					#else
						<img id="resume-upload-img" style="width:102px;height: 116px;" src="#springUrl('/campus/img/default-user.gif')" alt="">
					#end
					<a href="javascript:;" id="resume-upload" >上传头像</a>
					<p class="photo-notice">请上传500K以下照片<br>推荐像素100x114</p>
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>姓名</span>
					<input type="text" id="name" name="name" class="resume-input" required="" maxlength="10" value='$!{resumeBaseInfo.name}'>	
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>性别</span>
					#dictionary('sex','radio','{attributes:{id:"sex",name:"sex"}}',$!{resumeBaseInfo.sex})
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>出生日期</span>
					<input type="text" id="birthdayString" name="birthdayString" class="resume-input" required=""  value="#if($!{resumeBaseInfo.birthday}) $dateTool.format("yyyy.MM.dd", $!{resumeBaseInfo.birthday}) #end" onfocus="WdatePicker({readOnly:true,dateFmt:'yyyy.MM.dd',onpropertychange:removeFocus(this)})">
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>证件类型及号码</span>
					#dictionary('ZJLX','select','{attributes:{id:"idType",name:"idType",class:"resume-select"}}',$!{resumeBaseInfo.idType})
					<input type="text" id="idNo" name="idNo" class="resume-input resume-input-id" required=""  maxlength="20" value='$!{resumeBaseInfo.idNo}'>	
				</div>
				<div class="row">
					<span class="resume-tag">民族</span>
					#dictionary('MZ','select','{attributes:{id:"nationality",name:"nationality",class:"resume-select"}}',$!{resumeBaseInfo.nationality})
				</div>
				<div class="row">
					<span class="resume-tag">身高</span>
					<input type="text" id="height" name="height" class="resume-input-s" data-rule-digits="true" maxlength="5"  value='$!{resumeBaseInfo.height}'>
					<label for="J-height" class="resume-label">CM</label>
					<label id="J-height-error" class="error" for="J-height"></label>
				</div>
				<div class="row">
					<span class="resume-tag">体重</span>
					<input type="text" id="weight" name="weight" class="resume-input-s" data-rule-digits="true" maxlength="5" value='$!{resumeBaseInfo.weight}'>
					<label for="J-weight" class="resume-label">KG</label>
						<label id="J-weight-error" class="error" for="J-weight"></label>
				</div>
				
				<div class="row">
					<span class="resume-tag">婚姻状况</span>
					#dictionary('HYZK','radio','{attributes:{id:"maritalStatus",name:"maritalStatus"}}',$!{resumeBaseInfo.maritalStatus})
				</div>
				<div class="row">
					<span class="resume-tag">政治面貌</span>
					#dictionary('ZZMM','select','{attributes:{id:"politicsStatus",name:"politicsStatus",class:"resume-select"}}',$!{resumeBaseInfo.politicsStatus})
				</div>			
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>所在城市</span>
					#dictionary('CS','select','{appendEmptyOption:false,emptyOptionTitle:"",attributes:{id:"city",name:"city",class:"resume-select"}}',$!{resumeBaseInfo.city})
				</div>
				<div class="row">
					<span class="resume-tag">个人评价</span>
					<textarea name="selfEvaluation" id="selfEvaluation" class="resume-textarea"  maxlength="150" value="$!{resumeBaseInfo.selfEvaluation}"></textarea>
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>手机号码</span>
					<input type="text" id="mobile" name="mobile" class="resume-input" required=""  data-rule-digits="true"  maxlength="11" value="$!{resumeBaseInfo.mobile}">	
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>Email</span>
					<input type="email" id="email" name="email" class="resume-input" required=""  maxlength="30"  value="$!{resumeBaseInfo.email}">	
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>国籍</span>
					<input type="text" id="country" name="country" class="resume-input" required=""  maxlength="15"  value="$!{resumeBaseInfo.country}">
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>紧急联系人</span>
					<input type="text" id="emergencyContactPerson" name="emergencyContactPerson" class="resume-input" maxlength="10" required="" value="$!{resumeBaseInfo.emergencyContactPerson}">	
				</div>
				<div class="row">
					<span class="resume-tag"><i class="resume-required">*</i>紧急联系方式</span>
					<input type="text" id="emergencyContract" name="emergencyContract" class="resume-input" required="" maxlength="30" value="$!{resumeBaseInfo.emergencyContract}">	
				</div>
				<div class="row resume-submit">
					<span class="resume-tag">&nbsp;</span>
					<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
				</div>
			</form>
			</div>
		</div>
		<!-- 教育背景 -->
		<div id="J-edu-part" class="resume-part">
			<div class="resume-main-tit">
				教育背景 <span>（<i class="resume-required">*</i>必填）</span>
			</div>
			<table class="resume-exp-table" border="0" cellpadding="0" cellspacing="0">
				<tbody>
				<tr>
					<th>起止时间</th>
					<th>学校</th>
					<th>专业</th>
					<th>操作</th>
				</tr>
				#foreach($educationInfo in $!{resumeBaseInfo.educationInfos})
				<tr>
					<td>#if($!{educationInfo.startDate}) $dateTool.format("yyyy.MM", $!{educationInfo.startDate}) #end
						-#if($!{educationInfo.endDate}) $dateTool.format("yyyy.MM", $!{educationInfo.endDate}) #end</td>
					<td>$!{educationInfo.schoolName}</td>
					<td>$!{educationInfo.professionDictName}</td>
					<td>
						<a href="javascript:;"  onClick="javascript:modifyInfo($!{educationInfo.id},'educationInfo');">修改</a>
						<!-- a添加class btn-subform-delete -->
						<a class="btn-subform-delete" href="javascript:;" onClick="javascript:deleteInfo($!{educationInfo.id},$!{educationInfo.baseId},'educationInfo');">删除</a>
					</td>
    			</tr>				
			    #end	
			</tbody>
			</table>
			<div class="resume-main-content">
				<form action="/web/resume/saveEducationInfo" method="post">
				<!--	<input type="hidden" name="baseId" id="baseId" value=$!{resumeBaseInfo.id}>	-->
					<input type="hidden" name="id" id="educationInfoId">
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>起止时间</span>
						<input type="text" id="startDateString" name="startDateString" required="" class="resume-input-date" required="" onfocus="WdatePicker({readOnly:true,maxDate:'#F{$dp.$D(\'endDateString\')}',dateFmt:'yyyy.MM',onpropertychange:removeFocus(this)})">
						<label class="resume-label">至</label>
						<input type="text" id="endDateString" name="endDateString" required="" class="resume-input-date" required=""  onfocus="WdatePicker({readOnly:false,minDate:'#F{$dp.$D(\'startDateString\')}',dateFmt:'yyyy.MM',onpropertychange:removeFocus(this)})">
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>学校</span>
						<!--#dictionary('XX','select','{appendEmptyOption:false,emptyOptionTitle:"nihao",attributes:{id:"school",name:"school",class:"resume-select"}}',' ')-->
						<input type="text" id="schoolName" name="schoolName"  required="" class="resume-input">	
						<input type="hidden" id="school" name="school">
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>专业</span>
						#dictionary('zy','select','{appendEmptyOption:true,emptyOptionTitle:"",attributes:{id:"professionType",name:"professionType",class:"resume-select"}}')
						<!--#dictionary('zy01','select','{appendEmptyOption:false,emptyOptionTitle:"请选择",attributes:{id:"profession",name:"profession",class:"resume-select"}}')-->
						<select name="profession" id="profession" class="resume-select" required=""></select>
					</div>
					
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>专业名称</span>
						<input type="text" id="professionName" name="professionName"  required="" class="resume-input">
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>学历</span>					
						#dictionary('XL','select','{appendEmptyOption:false,emptyOptionTitle:"",attributes:{id:"educationBackground",name:"educationBackground",class:"resume-select"}}','XL02')
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>是否最高学历</span>
						#dictionary('ISORNOT','radio','{attributes:{id:"isHighestEducation",name:"isHighestEducation"}}')
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>学位</span>					
						#dictionary('XW','select','{appendEmptyOption:false,emptyOptionTitle:"",attributes:{id:"degree",name:"degree",class:"resume-select"}}','XW01')
					</div>
					<div class="row">
						<span class="resume-tag">主修课程</span>
						<textarea name="majorCourse" id="majorCourse" class="resume-textarea"></textarea>
					</div>
					<div class="row">
						<span class="resume-tag">证明人</span>
						<input type="text" id="certifier" name="certifier" class="resume-input">
					</div>
					<div class="row">
						<span class="resume-tag">证明人联系方式</span>
						<input type="text" id="certifierContactInfo" name="certifierContactInfo" class="resume-input">
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>海外学习经验</span>
						#dictionary('ISORNOT','radio','{attributes:{id:"overseasExperience",name:"overseasExperience"}}','0')
					</div>
					<div class="row">
						<!-- a添加class J-validate-continue -->
						<a href="javascript:;" class="resume-list-btn J-validate-continue">继续添加</a>
					</div>
					<div class="row resume-submit">
						<span class="resume-tag">&nbsp;</span>
						<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
					</div>
				</form>
			</div>
		</div>
		<!-- 专业证书 -->
		<div id="J-certificate-part" class="resume-part">
			<div class="resume-main-tit">
				专业证书 <span>（<i class="resume-required">*</i>必填，如无专业证书请直接点“下一步”）</span>
			</div>
			<table class="resume-exp-table" border="0" cellpadding="0" cellspacing="0">
				<tbody><tr>
					<th>证书名称</th>
					<th>获奖时间</th>
					<th>操作</th>
				</tr>
			   #foreach($resumeCertificate in $!{resumeBaseInfo.resumeCertificates})
				<tr>
					<td>$!{resumeCertificate.certificateTitle}</td>
					<td>#if($!{resumeCertificate.acquisitionTime}) $dateTool.format("yyyy.MM", $!{resumeCertificate.acquisitionTime}) #end</td>
					<td>
						<a href="javascript:;"  onClick="javascript:modifyInfo($!{resumeCertificate.id},'certificate');">修改</a>
						
						<a class="btn-subform-delete" href="javascript:;" onClick="javascript:deleteInfo($!{resumeCertificate.id},$!{resumeCertificate.baseId},'certificate');">删除</a>
    				</td>
				</tr>
			    #end
				</tbody>
			</table>
			<div class="resume-main-content">
				<form action="/web/resume/saveCertificate" method="post">
				 <input type="hidden" name="id" id="certId">
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>证书名称</span>
						<input type="text" id="certificateTitle" name="certificateTitle" class="resume-input" required="">
					</div>
					<div class="row">
						<span class="resume-tag">获奖时间</span>
						<input type="text" id="certAcquisitionTimeString" name="acquisitionTimeString" class="resume-input-date"  onfocus="WdatePicker({readOnly:true,dateFmt:'yyyy.MM',onpropertychange:removeFocus(this)})">	
					</div>
					<div class="row">
						<span class="resume-tag">证书描述</span>
						<textarea name="certificateDesc" id="certificateDesc" class="resume-textarea"></textarea>
					</div>
					<div class="row">
						<a href="javascript:;" class="resume-list-btn J-validate-continue">继续添加</a>
					</div>
					<div class="row resume-submit">
						<span class="resume-tag">&nbsp;</span>
						<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
						&nbsp;&nbsp;&nbsp;&nbsp;
						<a href="javascript:;" class="btn-red btn-nex">下一步</a>
					</div>
				</form>
			</div>
		</div>

		<!-- 奖励活动 -->
	<!--	<div id="J-reward-part" class="resume-part">
			<div class="resume-main-tit">
				奖励活动 <span>（<i class="resume-required">*</i>必填，如无奖励请直接点“下一步”）</span>
			</div>
			<div class="resume-main-content">
				<form id="rewardRecordForm" method="post">
					<input type="hidden" name="id" id="id" value=$!{resumeBaseInfo.rewardRecord.id}>
					<div class="row row-wrap">
						<span class="resume-tag">担任班级学生会干部</span>				
						#dictionary('GB','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"studentUnion",name:"studentUnion",class:"resume-select"}}',$!{resumeBaseInfo.rewardRecord.id})					
					</div>
					<div class="row row-wrap">
						<span class="resume-tag">校内活动</span>
						<textarea name="campusActivitie" id="campusActivitie" class="resume-textarea">$!{resumeBaseInfo.rewardRecord.campusActivitie}</textarea>
					</div>
					<div class="row row-wrap">
						<span class="resume-tag">其他奖励</span>
						<textarea name="otherIncentive" id="otherIncentive" class="resume-textarea">$!{resumeBaseInfo.rewardRecord.otherIncentive}</textarea>
					</div>
				</form>
			</div>
			<table class="resume-exp-table" border="0" cellpadding="0" cellspacing="0">
				<tbody><tr>
					<th>来源</th>
					<th>奖励内容</th>
					<th>获奖时间</th>
					<th>操作</th>
				</tr>		
				#foreach($bursary in $!{resumeBaseInfo.bursarys})
				<tr>
					<td>$!{bursary.sourceName}</td>
					<td>$!{bursary.level}</td>
					<td>#if($!{bursary.acquisitionTime}) $dateTool.format("yyyy.MM", $!{bursary.acquisitionTime}) #end</td>
					<td>
						<a href="javascript:;"  onClick="javascript:modifyInfo($!{bursary.id},'bursary');">修改</a>
						
						<a class="btn-subform-delete" href="javascript:;" onClick="javascript:deleteInfo($!{bursary.id},$!{bursary.baseId},'bursary');">删除</a>
    				</td>
				</tr>
			    #end
			</tbody></table>
			<div class="resume-main-content">
				<form action="/web/resume/saveBursary" method="post">	
					<input type="hidden" name="id" id="bursaryId">
					<div class="row">
						<span class="resume-tag">来源</span>						
						#dictionary('XJ','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"source",name:"source",class:"resume-select"}}')										
					</div>
					<div class="row">
						<span class="resume-tag">奖励内容</span>		
						<input type="text" id="level" name="level" class="resume-input" maxlength="50">
					</div>
					<div class="row">
						<span class="resume-tag">获奖时间</span>
						<input type="text" id="acquisitionTimeString" name="acquisitionTimeString" class="resume-input"  onfocus="WdatePicker({readOnly:true,dateFmt:'yyyy.MM'})">			
