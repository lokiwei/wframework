						<input type="text" id="toeic" name="toeic" class="resume-input-s" data-rule-digits="true" maxlength="5" value='$!{resumeBaseInfo.englishAbility.toeic}'>
					</div>
					<div class="row">
						<span class="resume-tag">SAT</span>
						<input type="text" id="sat" name="sat" class="resume-input-s" data-rule-digits="true" maxlength="5" value='$!{resumeBaseInfo.englishAbility.sat}'>
					</div>
				</form>
			</div>
			<div class="resume-subtitle">其他外语</div>
			<table class="resume-exp-table" border="0" cellpadding="0" cellspacing="0">
				<tbody><tr>
					<th>外语语种</th>
					<th>证书</th>
					<th>能力</th>
					<th>操作</th>
				</tr>				
				#foreach($foreignLanguage in $!{resumeBaseInfo.foreignLanguages})
				<tr>
					<td>$!{foreignLanguage.foreignLanguageName}</td>
					<td>$!{foreignLanguage.certificate}</td>
					<td>$!{foreignLanguage.listeningSpeakingAbilityName}</td>
					<td>
						<a href="javascript:;" onClick="javascript:modifyInfo($!{foreignLanguage.id},'foreignLanguage');">修改</a>
						<!-- a添加class btn-subform-delete -->
						<a class="btn-subform-delete" href="javascript:;" onClick="javascript:deleteInfo($!{foreignLanguage.id},$!{foreignLanguage.baseId},'foreignLanguage');">删除</a>
    				</td>
				</tr>
			    #end					
			</tbody></table>
			<div class="resume-main-content">
				<form action="/web/resume/saveForeignLanguage" method="post">
					<input type="hidden" name="id" id="foreignLanguageId">	
					<div class="row">
						<span class="resume-tag">外语语种</span>			
						#dictionary('wy','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"foreignLanguage",name:"foreignLanguage",class:"resume-select"}}')					
					</div>
					<div class="row">
						<span class="resume-tag">证书</span>
						<input type="text" id="certificate" name="certificate" class="resume-input" maxlength="30">
					</div>
					<div class="row">
						<span class="resume-tag">读写能力</span>
						#dictionary('NL','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"literacy",name:"literacy",class:"resume-select"}}')										
					</div>
					<div class="row">
						<span class="resume-tag">听说能力</span>
						#dictionary('NL','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"listeningSpeakingAbility",name:"listeningSpeakingAbility",class:"resume-select"}}')						
					</div>
					<div class="row">
						<a href="javascript:;" class="resume-list-btn J-validate-continue">继续添加</a>
					</div>
					<div class="row resume-submit">
						<span class="resume-tag">&nbsp;</span>
						<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
					     &nbsp;&nbsp;&nbsp;&nbsp;
			            <a href="javascript:;" class="btn-red btn-nex">下一步</a>
					</div>
				</form>
			</div>
		</div>

		<!-- 家庭成员 -->
		<div id="J-family-part" class="resume-part">
			<div class="resume-main-tit">
				家庭成员 <span>（<i class="resume-required">*</i>必填，如无家庭成员请直接点“下一步”）</span>
			</div>
			<table class="resume-exp-table" border="0" cellpadding="0" cellspacing="0">
				<tbody><tr>
					<th>姓名</th>
					<th>关系</th>
					<th>工作单位</th>
					<th>联系电话</th>
					<th>操作</th>
				</tr>
				#foreach($familyMember in $!{resumeBaseInfo.familyMembers})
					<tr>
					<td>$!{familyMember.name}</td>
					<td>$!{familyMember.relation}</td>
					<td>$!{familyMember.unit}</td>
					<td>$!{familyMember.duty}</td>
					<td>
						<a href="javascript:;" onClick="javascript:modifyInfo($!{familyMember.id},'familyMember');">修改</a>
						<!-- a添加class btn-subform-delete -->
						<a class="btn-subform-delete" href="javascript:;" onClick="javascript:deleteInfo($!{familyMember.id},$!{familyMember.baseId},'familyMember');">删除</a>
    				</td>
				</tr>
			    #end	
			</tbody></table>

			<div class="resume-main-content">
				<form action="/web/resume/saveFamilyMember" method="post">
					
					<input type="hidden" name="id" id="familyMemberId">	
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>姓名</span>
						<input type="text" id="familyMemberName" name="name" class="resume-input" required="">
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>关系</span>
						<input type="text" id="relation" name="relation" class="resume-input" required="">
					</div>
					<div class="row">
						<span class="resume-tag">工作单位</span>
						<input type="text" id="unit" name="unit" class="resume-input">
					</div>
					<div class="row">
						<span class="resume-tag">职务</span>
						<input type="text" id="duty" name="duty" class="resume-input">
					</div>
					<div class="row">
						<span class="resume-tag">联系电话</span>
						<input type="text" id="contactNumber" name="contactNumber"  data-rule-digits="true" class="resume-input">
					</div>
					<div class="row">
						<!-- a添加class J-validate-continue -->
						<a href="javascript:;" class="resume-list-btn J-validate-continue">继续添加</a>
					</div>
					<div class="row resume-submit">
						<span class="resume-tag">&nbsp;</span>
						<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
						&nbsp;&nbsp;&nbsp;&nbsp;
			            <a href="javascript:;" class="btn-red btn-nex">下一步</a>
					</div>
				</form>
			</div>
		</div>

		<!-- 其他信息 -->
		<div id="J-other-part" class="resume-part">
			<div class="resume-main-tit">
				其他信息 <span>（<i class="resume-required">*</i>必填）</span>
			</div>
			<div class="resume-main-content">
				<form action="/web/resume/saveOtherInfo" method="post">
					
					<input type="hidden" name="id" id="id" value=$!{resumeBaseInfo.otherInfo.id}>
				<!--	<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>申请类别</span>
						<input type="radio" id="J-other-graduate-yes" name="J-other-graduate" value="0" class="resume-radio" checked="">
						<label for="J-other-graduate-yes" class="resume-label">2015年毕业</label>
						<input type="radio" id="J-other-graduate-no" name="J-other-graduate" value="1" class="resume-radio">
						<label for="J-other-graduate-no" class="resume-label">非2015年毕业</label>
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>毕业月份</span>
						<input type="text" id="J-other-date" name="J-other-date" class="resume-input" required="">
						<input type="text" id="expectedEnterTime" name="expectedEnterTime" class="resume-input" required="" onfocus="WdatePicker({readOnly:true,dateFmt:'yyyy.MM.dd'})">						
					</div>-->
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>期望工作城市</span>
					    <input type="text" id="hopeCity" name="hopeCity" class="resume-input" required="" value="$!{resumeBaseInfo.otherInfo.hopeCity}" >						
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>是否有亲友在京东</span>
						#if($!{resumeBaseInfo.otherInfo.isRelationJd}!="null"&&$!{resumeBaseInfo.otherInfo.isRelationJd}!="")								
						    
							#dictionary('ISORNOT','radio','{attributes:{id:"isRelationJd",name:"isRelationJd"}}',$!{resumeBaseInfo.otherInfo.isRelationJd})					
						#else								
						    #dictionary('ISORNOT','radio','{attributes:{id:"isRelationJd",name:"isRelationJd"}}','0')					
						#end
					</div>
					<div class="row">
						<span class="resume-tag">招聘信息来源</span>					
						#dictionary('ly','select','{appendEmptyOption:true,emptyOptionTitle:"请选择",attributes:{id:"infoSource",name:"infoSource",class:"resume-select"}}',$!{resumeBaseInfo.otherInfo.infoSource})				
					</div>
					<div class="row">
						<span class="resume-tag"><i class="resume-required">*</i>可入职时间</span>
						<input type="text" id="expectedEnterTimeString" name="expectedEnterTimeString" class="resume-input" required="" value="#if($!{resumeBaseInfo.otherInfo.expectedEnterTime}) $dateTool.format("yyyy.MM.dd", $!{resumeBaseInfo.otherInfo.expectedEnterTime}) #end" onfocus="WdatePicker({readOnly:true,dateFmt:'yyyy.MM.dd',onpropertychange:removeFocus(this)})">	
					</div>
					<div class="row resume-submit">
						<span class="resume-tag">&nbsp;</span>
						<a href="javascript:;" class="btn-red btn-save-next">保存并下一步</a>
					</div>
				</form>
			</div>
		</div>

		<!--S 上传附件 -->
		#parse("/web/resume/resume-jss.vm")
		<!--E 上传附件 -->
	</div>
</div>

