#set($layout = "layout/defaultNew.vm")
<script type="text/javascript" src="#springUrl('')/js/manage/func/application_index.js"></script>

<div class="filtrate-wrap">
	<ul class="filtrate">
		<li class="row">
			<div class="txt">应用名称</div>
			<div class="con">
				<input class="title" id="applicationName" type="text"/>
			</div>
		</li>
		<li class="row status">
			<div class="txt"></div>
			<div class="con">
				<div id="search" class="btn-red btn-lg">查询</div>
			</div>
		</li>
		<li class="clear"></li>
	</ul>
</div>
<div class="summary">
	<div class="btn-red add" data-toggle="modal" data-target="#J-modal-plan">新建</div>
	<div class="clear"></div>
	<table cellpadding="0"  cellspacing="0" border="0" class="dataTable" id="applicationTable" width="100%">
	</table>
</div>